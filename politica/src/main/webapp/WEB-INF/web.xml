<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>UsuarioServletSA</servlet-name>
        <servlet-class>servlet.UsuarioServletSA</servlet-class>
        <multipart-config>
            <max-file-size>418018841</max-file-size>
            <max-request-size>418018841</max-request-size>
            <file-size-threshold>418018841</file-size-threshold>
        </multipart-config>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PublicacaoServletSA</servlet-name>
        <servlet-class>servlet.PublicacaoServletSA</servlet-class>
        <multipart-config>
            <max-file-size>418018841</max-file-size>
            <max-request-size>418018841</max-request-size>
            <file-size-threshold>418018841</file-size-threshold>
        </multipart-config>
    </servlet>
    <servlet>
        <servlet-name>UsuarioServletD</servlet-name>
        <servlet-class>servlet.UsuarioServletD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioServletLogout</servlet-name>
        <servlet-class>servlet.UsuarioServletLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PublicacaoServletD</servlet-name>
        <servlet-class>servlet.PublicacaoServletD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>ComentarioServletSA</servlet-name>
        <servlet-class>servlet.ComentarioServletSA</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ComentarioServletD</servlet-name>
        <servlet-class>servlet.ComentarioServletD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioServletBuscar</servlet-name>
        <servlet-class>servlet.UsuarioServletBuscar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AvaliacaoServletS</servlet-name>
        <servlet-class>servlet.AvaliacaoServletS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PublicacaoServletSA</servlet-name>
        <url-pattern>/PublicacaoServletSA</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServletD</servlet-name>
        <url-pattern>/UsuarioServletD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServletSA</servlet-name>
        <url-pattern>/UsuarioServletSA</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServletLogout</servlet-name>
        <url-pattern>/UsuarioServletLogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PublicacaoServletD</servlet-name>
        <url-pattern>/PublicacaoServletD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ComentarioServletSA</servlet-name>
        <url-pattern>/ComentarioServletSA</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ComentarioServletD</servlet-name>
        <url-pattern>/ComentarioServletD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServletBuscar</servlet-name>
        <url-pattern>/UsuarioServletBuscar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AvaliacaoServletS</servlet-name>
        <url-pattern>/AvaliacaoServletS</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>faces/index.xhtml</welcome-file>
    </welcome-file-list>
</web-app>
